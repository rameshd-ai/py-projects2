{
    "ComponentFormat": [
        {
            "FormatId": 867409,
            "FormatName": "Spa-Module-Navigation",
            "FormatKey": "spa-module-navigation",
            "FormatPrefix": "",
            "FormatContent": "{{#each ComponentRecordJson.spa-module-navigation}}\n<div class=\"module-nav dynamiccomponenteditenable\" %%componentRecordEditable%% id=\"module-nav\">\n    <div class=\"module-nav__dropdown\">\n        <div class=\"title title--s module-nav__title\" tabindex=\"0\">{{data.module-name}}</div> \n        <a class=\"module-nav__toggler\" data-uk-toggle=\"target: #module-nav; cls: is-toggle-active\" href=\"javascript:void(0);\"> \n            <span class=\"js-module-txt\">{{Child.spa-module-navigation-pages.0.page-name}}</span> \n            <span class=\"template-icon template-icon-arrow-narrow-down\"></span> \n        </a>\n    </div>\n    <ul class=\"module-nav__tabs js-modulelinks\">\n        {{#each Child.spa-module-navigation-pages}}\n        <li class=\"modulelink dynamiccomponenteditenable\" %%componentRecordEditable%%><a href=\"{{data.page-link}}\" {{IfCond data.open-url-in-a-new-tab[].0 '=' 'Yes'}}   target=\"_blank\"  {{/IfCond}} class=\"nav\">{{data.page-name}}</a></li>\n        {{/each}}\n    </ul>\n</div>\n{{/each}}",
            "FormatPostfix": "",
            "Status": 1,
            "ParentId": 0,
            "IsPublished": 0,
