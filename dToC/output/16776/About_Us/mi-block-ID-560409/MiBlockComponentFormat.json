    "ComponentFormat": [
        {
            "FormatId": 881814,
            "FormatName": "Hero-banner-with-title",
            "FormatKey": "hero-banner-with-title",
            "FormatPrefix": "",
            "FormatContent": "{{#each ComponentRecordJson.hero-banner-with-title}}\n<!-- Header 30 Starts -->\n<div class=\"hr30-widget {{IfCond data.center-content-vertically[].0 '=' 'Yes'}} hr30-widget--middle {{/IfCond}} {{IfCond data.wrap-banner-in-container[].0 '=' 'Yes'}} hr30-widget--contained {{/IfCond}}\">\n    <div class=\"uk-position-relative uk-visible-toggle\" tabindex=\"-1\" data-uk-slider>\n        <div class=\"uk-slider-items uk-child-width-1-1\">\n             {{#each data.banner-image}}\n                <div class=\"hr30-widget__media\">\n                    <img src=\"{{OriginalImagePath}}\" alt=\"{{../data.banner-image-alt-text}}\" width=\"1440\" height=\"550\" tabindex=\"0\" data-nodeferimage=\"true\">\n                </div>\n            {{/each}}\n        </div>\n        {{IfCondObj data.banner-image '>' '1'  }} \n        <div class=\"slider-controls\">\n            <div class=\"slider-controls__arrows uk-flex uk-flex-middle uk-flex-between\">\n                <a class=\"slider-controls__slidenav slider-controls__slidenav--left uk-icon uk-slidenav-previous uk-slidenav\" href=\"#\" data-uk-slider-item=\"previous\">\n                    <span class=\"template-icon template-icon-chevron-left-thin\"></span>\n                    <span class=\"sr-only\">Go to Previous Slide of {{data.section-title}}</span>\n                </a>\n\n                <a class=\"slider-controls__slidenav slider-controls__slidenav--right uk-icon uk-slidenav-next uk-slidenav\" href=\"#\" data-uk-slider-item=\"next\">\n                    <span class=\"template-icon template-icon-chevron-right-thin\"></span>\n                    <span class=\"sr-only\">Go to Next Slide of {{data.section-title}}</span>\n                </a>\n            </div>\n        </div>\n        {{/IfCondObj}}\n    </div>\n    <div class=\"hr30-widget__content {{IfCond data.use-a-larger-container-for-content[].0 '=' 'Yes'}} hr30-widget__content--bigger {{/IfCond}}\">\n        <div class=\"title title--xxxl hr30-widget__title dynamiccomponenteditenable\" %%componentRecordEditable%% tabindex=\"0\">{{data.title}}</div>\n    </div>\n</div>\n<!-- Header 30 Ends -->\n{{/each}}",
