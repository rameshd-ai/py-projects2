{
    "ComponentFormat": [
        {
            "FormatId": 874273,
            "FormatName": "Venue accordian",
            "FormatKey": "venue-accordian",
            "FormatPrefix": "",
            "FormatContent": "\n<div class=\"inner-padding venue-accordian {{IfCond data.set-top-padding-to-zero[].0 '=' 'Yes'}} uk-padding-remove-top {{/IfCond}} {{IfCond data.set-bottom-padding-to-zero[].0 '=' 'Yes'}} uk-padding-remove-bottom {{/IfCond}}\">\n  <div class=\"uk-container snippet__container\">\n    {{#each ComponentRecordJson.venue-accordian}}\n    <div class=\"uk-grid gutter-xl snippet__zigzag snippet__zigzag--fullwidth\" >\n      <div class=\"uk-width-auto@m snippet__col\">\n        <div class=\"snippet__media\">\n          <img src=\"{{data.image.0.OriginalImagePath}}\" alt=\"{{data.alt-text}}\" width=\"650\" height=\"500\" data-name=\"contactnearbyboxes-image\" class=\"dynamiccomponenteditenable\" tabindex=\"0\">\n        </div>\n      </div>\n      <div class=\"uk-width-expand@m snippet__col\">\n        {{IfCond data.title 'notempty' ''}}\n        <h2 class=\"snippet__title dynamiccomponenteditenable\"  tabindex=\"0\" data-name=\"contactnearbyboxes-title\" %%componentRecordEditable%%>{{data.title}}</h2>\n        {{/IfCond}}\n\n        {{IfCond data.description 'notempty' ''}}\n        <div class=\"snippet__desc\" tabindex=\"0\" data-name=\"contactnearbyboxes-description\">\n          {{{data.description}}}\n        </div>\n        {{/IfCond}}\n\n        {{IfCondObj Child.accordian-list-venue '>' '0'  }} \n        <ul class=\"accoradian snippet__accoradian snippet__accoradian--nearby-boxes\" data-uk-accordion data-name=\"contactnearbyboxes-accordion\">\n          {{#each child.accordian-list-venue}}\n          {{IfCond this.ParentId  '='  ../this.Id}}\n          <li class=\"dynamiccomponenteditenable\" %%componentRecordEditable%%>\n            <a class=\"uk-accordion-title uk-accordion-title--spaservices\" href=\"#\">{{data.title}}</a>\n            <div class=\"uk-accordion-content uk-accordion-content--spaservices\" tabindex=\"0\" hidden>{{{data.description}}}</div>\n          </li>\n          {{/IfCond}}\n          {{/each}}\n        </ul>\n        {{/IfCondObj}}\n\n        {{IfCond data.maincta-text 'notempty' '' 'and' data.maincta-link 'notempty' ''}}\n        <div class=\"snippet__actions\"> \n          <a role=\"button\" target=\"_blank\" href=\"{{data.maincta-link}}\" class=\"uk-button uk-button-primary\" data-name=\"contactnearbyboxes-cta\">{{data.maincta-text}} <span class=\"sr-only\">{{data.title}}</span></a>\n        </div>\n        {{/IfCond}}\n      </div>\n    </div>\n    {{/each}}\n  </div>\n</div>\n",
            "FormatPostfix": "",
            "Status": 1,
            "ParentId": 0,
            "IsPublished": 0,
            "FormatPrefixTablet": "",
            "FormatContentTablet": "",
            "FormatPostfixTablet": "",
            "FormatPrefixMobile": "",
            "FormatContentMobile": "",
            "FormatPostfixMobile": "",
            "FormatPrefixAMP": "",
            "FormatContentAMP": "",
            "FormatPostfixAMP": "",
            "PageConfigurationId": 0,
            "IsExcludeFromJsonLD": false,
            "Category": null
        }
    ]
}